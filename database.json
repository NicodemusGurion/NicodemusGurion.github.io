---
layout: null
permalink: /database.json
---
{{- site.time -}}
{% assign wordList = ""  %}
{% assign valList = "" %}
{% for post in site.posts  %}
{%   assign content = post.content | replace: "&nbsp;", " "  %}
{%   assign words = content | strip_html | strip_newlines | slugify: "latin" | split: "-" %} 
{%   for word in words  %}
{%    unless wordList contains word  %}
{%       assign wordList = wordList | append: "[" | append: word | append: "] " %}
{%     else %}
{%     endunless %}
{%   endfor %}
{% endfor %}
{{- wordList -}}
