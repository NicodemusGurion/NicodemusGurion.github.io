---
layout: null
permalink: /database.json
mydb:
  word: "Foobar"
  page: "pageurl"
  count: 16
---
{{- site.time -}}
{{- mydb | jsonify -}}
{% assign searchdb = "" | split: "," %}
{% for post in site.posts %}
  {% assign content = post.content | replace: "&nbsp;", " " %}
  {% assign words = content | strip_html | strip_newlines | slugify: "latin" | split: "-" %}
  {% for word in words %}
    {% unless searchdb contains word %}
      {% assign searchdb = searchdb | push: word %}
    {% endunless %}
  {% endfor %}
{% endfor %}
